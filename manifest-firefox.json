{
    "manifest_version": 2,
    "name": "Svelte Hello World Extension",
    "version": "1.0",
    "description": "A simple extension using Svelte.",
    "browser_action": {
      "default_popup": "popup/dist/index.html",
      "default_title": "Hello World!"
    },
    "permissions": [
      "storage"
    ],
    "background": {
      "scripts": ["popup/background.js"]
    }
  }
  